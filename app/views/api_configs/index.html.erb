<h1>Listing api_configs</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Password</th>
      <th>Security token</th>
      <th>Client</th>
      <th>Client secret</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_configs.each do |api_config| %>
      <tr>
        <td><%= api_config.username %></td>
        <td><%= api_config.password %></td>
        <td><%= api_config.security_token %></td>
        <td><%= api_config.client_id %></td>
        <td><%= api_config.client_secret %></td>
        <td><%= link_to 'Show', api_config %></td>
        <td><%= link_to 'Edit', edit_api_config_path(api_config) %></td>
        <td><%= link_to 'Destroy', api_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api config', new_api_config_path %>
